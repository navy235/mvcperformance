@model string
@{
    var name = "Password";
}
@Html.PasswordFor(m => m)

<script type="text/javascript">
    var @name={
        id:'@name',
        check_id:'check_@name',
        clear:function(){
            var self=@name;
            document.getElementById(self.id).value='';
        }
    }
</script>
